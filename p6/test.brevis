void test_write() {
    print <- "Hello world!";
    print <- "\n";
    print <- 1675437;
    print <- "\n";
    print <- false;
    print <- "\n";
    print <- true;
    print <- "\n";
}

void test_assign(){
    integer a;
    integer anotherA;
    a = 12;
    anotherA = a;
    print <- a;
    print <- "\n";
    print <- anotherA;
    print <- "\n";
    b = 13;
    print <- b;
    print <- "\n";
    anotherA = b;
    print <- anotherA;
    print <- "\n";
    t = true;
    print <- t;
    print <- "\n";
}

void test_arithmitic(){
    integer a;
    boolean b;
    a = -12;
    print <- a;
    print <- "\n";
    b = false;
    print <- \b;
    print <- "\n";
    print <- 5 + -8;
    print <- "\n";
    print <- 1024 - 768;
    print <- "\n";
    print <- 2 * 2 * 2 * 2 * (5-1);
    print <- "\n";
    print <- 65 / 32;
    print <- "\n";
}

void test_equality(){
    print <- 3 == 3;
    print <- "\n";
    print <- "hello" == "world";
    print <- "\n";
    print <- 3 \= (3*5);
    print <- "\n";
    print <- "hello" \= "hello";
    print <- "\n";
}

void test_relational(){
    print <- 1 < 3;
    print <- "\n";
    print <- 100 < 2;
    print <- "\n";
    print <- 1 <= 1;
    print <- "\n";
    print <- 3 <= 2;
    print <- "\n";
    print <- 5 > 2;
    print <- "\n";
    print <- 2 > 5;
    print <- "\n";
    print <- 5 >= 5;
    print <- "\n";
    print <- 4 >= 5;
    print <- "\n";
}

void test_logical(){
    print <- true && true;
    print <- "\n";
    print <- true && 2 > 5;
    print <- "\n";
    print <- true || false;
    print <- "\n";
    print <- false || 2 > 5;
    print <- "\n";
}

void test_stmt(){
    integer a;
    a = 2;
    a++;
    a++;
    a--;
    print <- a;
    print <- "\n";
    //scan -> a;
    //print <- a;
    //print <- "\n";
}

void test_conditional(){
    integer a;
    a = 6;
    if (a <= 10) {
        print <- "a less than equals 10\n";
    }
    print <- "a is: ";
    print <- a;
    print <- "\n";
    a = 64;
    print <- a;
    if (a <= 100){
        print <- " less than equals 100\n";
    } else {
        print <- " grater than 100\n";
    }
    print <- "now print even numbers from 2 to 10\n";
    a = 2;
    while (a <= 10){
        print <- a;
        print <- " ";
        a++;
        a++;
    }
    print <- "\n";
}

void greetings(boolean isMorning){
    if (isMorning){
        print <- "Hello World!\n";
    } else {
        print <- "Goodnight World!\n";
    }
    
}

integer calculate_plus(integer num1, integer num2){
    return num1 + num2;
}

void test_call(){
    greetings(true);
    greetings(false);
    print <- calculate_plus(128, 128);
    print <- "\n";
}

void test_all(){
    test_write();
    test_assign();
    test_arithmitic();
    test_equality();
    test_relational();
    test_logical();
    test_stmt();
    test_conditional();
    test_call();
}

void main(){
    test_all();
}
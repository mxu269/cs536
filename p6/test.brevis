integer a;
integer b;
boolean t;
boolean f;

void test_write() {
    print <- "Hello world!";
    print <- "\n";
    print <- 1675437;
    print <- "\n";
    print <- false;
    print <- "\n";
    print <- true;
    print <- "\n";
}

void test_assign(){
    integer a;
    integer anotherA;
    a = 12;
    anotherA = a;
    print <- a;
    print <- "\n";
    print <- anotherA;
    print <- "\n";
    b = 13;
    print <- b;
    print <- "\n";
    anotherA = b;
    print <- anotherA;
    print <- "\n";
    t = true;
    print <- t;
    print <- "\n";
}

void test_arithmitic(){
    integer a;
    boolean b;
    a = -12;
    print <- a;
    print <- "\n";
    b = false;
    print <- \b;
    print <- "\n";
    print <- 5 + -8;
    print <- "\n";
    print <- 1024 - 768;
    print <- "\n";
    print <- 2 * 2 * 2 * 2 * (5-1);
    print <- "\n";
    print <- 65 / 32;
    print <- "\n";
}

void test_equality(){
    print <- 3 == 3;
    print <- "\n";
    print <- "hello" == "world";
    print <- "\n";
    print <- 3 \= (3*5);
    print <- "\n";
    print <- "hello" \= "hello";
    print <- "\n";
}

void test_relational(){
    print <- 1 < 3;
    print <- "\n";
    print <- 100 < 2;
    print <- "\n";
    print <- 1 <= 1;
    print <- "\n";
    print <- 3 <= 2;
    print <- "\n";
    print <- 5 > 2;
    print <- "\n";
    print <- 2 > 5;
    print <- "\n";
    print <- 5 >= 5;
    print <- "\n";
    print <- 4 >= 5;
    print <- "\n";
}

void main(){
    print <- true && true;
    print <- "\n";
    print <- true && 2 > 5;
    print <- "\n";
    print <- true || false;
    print <- "\n";
    print <- false || 2 > 5;
    print <- "\n";
}